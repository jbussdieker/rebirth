<h1>Listing transaction_methods</h1>

<table>
  <tr>
    <th>Caption</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @transaction_methods.each do |transaction_method| %>
  <tr>
    <td><%= transaction_method.caption %></td>
    <td><%= link_to 'Show', transaction_method %></td>
    <td><%= link_to 'Edit', edit_transaction_method_path(transaction_method) %></td>
    <td><%= link_to 'Destroy', transaction_method, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

